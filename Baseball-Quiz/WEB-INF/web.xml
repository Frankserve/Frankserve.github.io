<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0"
		 metadata-complete="true">

    <display-name>101 Tutorial</display-name>
	
	<servlet>
		<servlet-name>baseball</servlet-name>
		<servlet-class>com.example.web.QuizServlet</servlet-class>
	</servlet>
        
	<servlet-mapping>
		<servlet-name>baseball</servlet-name>
		<url-pattern>/Select</url-pattern>
	</servlet-mapping>

    <welcome-file-list>
        <welcome-file>quiz-cpbl.html</welcome-file>
    </welcome-file-list>
    
 </web-app>